QueryProductStock *-->  "0..*" RelatedParty : relatedParty

QueryProductStock *-->  "0..*" ProductStock : queryProductStockItem

QueryProductStock *-->  "0..1" ProductStock : searchCriteria

ProductStock *-->  "0..1" ProductRefOrValue : stockedProduct

