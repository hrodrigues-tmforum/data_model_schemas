ProductStock *-->  "0..*" ResourceRef : resource

ProductStockStatusType <-left[hidden]- ProductStock

ProductStockUsageType <-left[hidden]- ProductStockStatusType

ProductRefOrValue *-right-> "0..*" ProductRefOrValue : product

