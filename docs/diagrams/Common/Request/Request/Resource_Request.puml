@startuml
hide circle
hide methods
hide stereotype
show <<Enumeration>> stereotype
skinparam class {
   BackgroundColor<<Enumeration>> #E6F5F7
   BackgroundColor<<Ref>> #FFFFE0
   BackgroundColor<<Pivot>> #FFFFFFF
   BackgroundColor<<SimpleType>> #E2F0DA
   BackgroundColor #FCF2E3
}

class Request <<Pivot>> {
    body : String [1]
    method : String
    to : String
}

class HeaderItem  {
    name : String [1]
    value : String [1]
}

Request *-->  "1..*" HeaderItem : header

@enduml