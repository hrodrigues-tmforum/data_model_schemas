@startuml
hide circle
hide methods
hide stereotype
show <<Enumeration>> stereotype
skinparam class {
   BackgroundColor<<Enumeration>> #E6F5F7
   BackgroundColor<<Ref>> #FFFFE0
   BackgroundColor<<Pivot>> #FFFFFFF
   BackgroundColor<<SimpleType>> #E2F0DA
   BackgroundColor #FCF2E3
}

class Privilege <<Pivot>> {
    action : String
    function : String
    href : Uri
    id : String [1]
    name : String
    @baseType : String
    @referredType : String
    @schemaLocation : Uri
    @type : String
}

class AssetGroup  {
    href : Uri
    id : String
    @baseType : String
    @schemaLocation : Uri
    @type : String
}

Privilege *-->  "0..1" AssetGroup : managedAssetGroup

@enduml