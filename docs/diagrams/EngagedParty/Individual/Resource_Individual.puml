    href : Uri
    id : String
    validFor : TimePeriod
    @baseType : String
    @schemaLocation : Uri
    @type : String
}

class IndividualIdentification  {
    href : Uri
    id : String
    identificationId : String
    identificationType : String
    issuingAuthority : String
    issuingDate : DateTime
    validFor : TimePeriod
    @baseType : String
    @schemaLocation : Uri
    @type : String
